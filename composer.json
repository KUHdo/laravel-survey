{
    "name": "kuhdo/laravel-survey",
    "description": "Package for create surveys in laravel",
    "version": "0.2.0",
    "license": "license",
    "authors": [
        {
            "name": "Eduard Malyj",
            "email": "eduard.malyj@kuhdo.de",
            "homepage": "https://www.kuhdo.de"
        },
        {
            "name": "Arne Bartelt",
            "email": "arne.bartelt@kuhdo.de",
            "homepage": "https://www.kuhdo.de"
        }],
    "homepage": "https://bitbucket.org/kuhdo/laravel-survey",
    "keywords": [
        "Laravel",
        "Survey",
        "qa",
        "question",
        "answer"
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^v6.13.0",
        "sempro/phpunit-pretty-print": "^1.4",
        "squizlabs/php_codesniffer": "3.*"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "KUHdo\\Survey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KUHdo\\Survey\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KUHdo\\Survey\\SurveyServiceProvider"
            ],
            "aliases": {
                "Survey": "KUHdo\\Survey\\Facades\\Survey"
            }
        }
    }
}
